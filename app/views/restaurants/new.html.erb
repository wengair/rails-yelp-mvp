<div class="container">
  <h1>Restaurants#new</h1>
  <p>Find me in app/views/restaurants/new.html.erb</p>
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category, as: :select, collection: Restaurant::CATE  %>
    <%= f.input :phone_number %>
    <%= f.submit "add a review", class: "btn btn-primary" %>
  <% end %>
</div>
